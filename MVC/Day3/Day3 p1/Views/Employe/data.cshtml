        <td>
            @Html.DisplayFor(modelItem => item.EmpFname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpLname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpSalary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpHDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CtyID)
        </td>
        <td>
